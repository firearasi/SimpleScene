<!-- General Login Scripts -->

<script>
    $(document).ready(function(){

        $('.toggle-debug-data').click( function(e) {
            $('.debug-data').toggleClass('hidden');
        });
        $('.toggle-parent').click(function(e){
            console.log($(this).attr('data-label'));
            if ($(this).attr('data-label') === 'Close' || typeof $(this).attr('data-label') === 'undefined') {
                $(this).attr('data-label', $(this).html());
                $(this).html('Close');  
            } else {
                $(this).html($(this).attr('data-label'));
                $(this).attr('data-label', 'Close');

            }
            
            $(this).parent().toggleClass('open');
        });

        

        function openSignupOverlayForSocial() {
            var overlay = $("#signup-overlay");
            overlay.fadeIn(200);
            overlay.css("background-color", "rgba(42, 48, 58, 0.95)");
            overlay.find(".title").html('<span class="highlight">Step 1:</span> Create an account or login');
            overlay.find("#signup").append('<input type="hidden" value="true" name="skip-ver">');

            overlay.find('.pick_username_form').removeClass('hidden');
            overlay.find('.social_login_label').addClass('hidden');
            overlay.find('.regular-signup').addClass('hidden');
        }


        //check to see the new social login
        function checkNewSocialLogin(data){

            data["action"] = "checkNewUser";

            $.ajax({
                type: "POST",
                url: "/post.php",
                data: data,
                async: true,
                dataType: "json",
                success: function( response ){
                    //if logged in
                    if (response['loggedin'] === true) {
                        window.location.reload();
                    } else {
                        //if new user - ask user to pick a username
                        if (response['new'] === true) {

                            $('.pick_username').attr('data-social_login_type', response['social_data']['type']);

                            if (response['available_username']){
                                $(".pick_username").val(response['available_username']);
                            }
                            $('.pick_username_form .error_messages').addClass('hidden');
                            var add_missing_email = $('.add_missing_email');
                            add_missing_email.addClass('hidden');
                            if (response['needs_email'] === true) {
                                add_missing_email.removeClass('hidden');
                            }

                            //$(".pick_username_form").removeClass("hidden");
                            $('.social_login_btn.google').removeClass('waiting');
                            $('.social_login_btn.facebook').removeClass('waiting');
                            openSignupOverlayForSocial();
                        }
                    }
                    console.log(response);
                },
                error: function( response ){
                    console.log("error");
                    return 'error';
                    $('.social_login_btn.google').removeClass('waiting');
                    $('.social_login_btn.facebook').removeClass('waiting');
                }
            });
        }

        //try to create account with the username chosen by the user
        function createAccountWithUsername(data) {

            var slo_error = $('.social_login_options .error_messages');
            console.log(99999);
            data['action'] = "checkNewUser";
            $.ajax({
                type: "POST",
                url: "/post.php",
                data: data,
                async: true,
                dataType: "json",
                success: function( response ){
                    if (response['loggedin'] === true){
                        window.location.reload();
                    } else {
                        console.log(response);

                        $(slo_error).html(" ");
                        if (typeof response['errors'] != 'undefined'){
                            response['errors'].forEach(function(res_error) {
                                $(slo_error).removeClass('hidden');
                                $(slo_error).append('<div class="alert alert-error m10">' + res_error + '</div>')
                            });
                        }
                        return false;
                    }
                },
                error: function( response ){

                    $('.social_login_btn.google').removeClass('waiting');
                    $('.social_login_btn.facebook').removeClass('waiting');
                    
                    $(slo_error).html(" ");
                    if (typeof response['errors'] != 'undefined'){
                        response['errors'].forEach(function(res_error) {
                            $(slo_error).removeClass('hidden');
                            $(slo_error).append('<div class="alert alert-error m10">' + res_error + '</div>')
                        });
                    }
                    return false;
                }
            });
        }

        //click handler for the facebook button
        $(document).on('click','.social_login_btn.facebook', function(e) {

            var waiting = $('.social_login_btn.facebook').hasClass('waiting');
            if (waiting === false) {
                $('.social_login_btn.facebook').addClass('waiting');
                $('.social_login_options').removeClass('active');
                $(e.target).closest('.social_login_options').addClass('active');

                //if FB object is available
                if (typeof FB != 'undefined'){
                    FB.getLoginStatus(function(response){

                        //user is connected
                        if (response.status == 'connected'){
                            var authResponse = response.authResponse;
                            data = {
                                "social_login_type": "facebook",
                                "id": authResponse.userID,
                                "access_token": authResponse.accessToken
                            }
                            checkNewSocialLogin(data);
                        } else {
                            //user is not connected - prompt user to log in
                            FB.login(function(response) {
                                var authResponse = response.authResponse;
                                data = {
                                    "social_login_type": "facebook",
                                    "id": authResponse.userID,
                                    "access_token": authResponse.accessToken
                                }
                                checkNewSocialLogin(data);
                            });
                        }
                    });
                } else {
                    console.log('facebook not loaded');
                    $('.social_login_btn.google').removeClass('waiting');
                    $('.social_login_btn.facebook').removeClass('waiting');
                }
            }
            
        });

        element = document.querySelector('.social_login_btn.google');        

        $(document).on('click', '.social_login_btn.google', function(e) {

            var waiting = $('.social_login_btn.google').hasClass('waiting');
            if (waiting === false) {
                $('.social_login_btn.google').addClass('waiting');
                $('.social_login_options').removeClass('active');
                $(e.target).closest('.social_login_options').addClass('active');

                auth2 = gapi.auth2.getAuthInstance();
                
                auth2.signIn().then(
                    function (googleUser){
                        $('.social_login_options').removeClass('active');
                        $(element).closest('.social_login_options').addClass('active');

                        var authResponse = googleUser.getAuthResponse();
                        if (authResponse) {
                            data = {
                                "social_login_type": "google",
                                "id_token": authResponse.id_token,
                                "access_token": authResponse.access_token
                            }
                            checkNewSocialLogin(data);
                        } 

                        console.log('Signed in: ' + googleUser.getBasicProfile().getName());
                }, function (error){
                    console.log('Sign-in error', error);
                    $('.social_login_btn.google').removeClass('waiting');
                    $('.social_login_btn.facebook').removeClass('waiting');
                });
            }
        });
        

        //submit username to try and attach login data
        $('body').on('click', ".pick_username_form input[type='submit']", function(e){
            e.preventDefault();

            var slo = $(e.target).closest('.social_login_options');
            var username = $(slo).find(".pick_username");
            var email = $(slo).find('.pick_email');
            var agree_terms = $(slo).find(".agree_check");
            var agree_subscribe = $(slo).find(".subscribe_check");
            var slo_error = $(slo).find('.error_messages');
            
            // console.log(11111);
            // console.log($(username).attr('data-social_login_type'));
            // console.log(username);
            // console.log(slo);
            // console.log($(e.target));

            switch ($(username).attr('data-social_login_type')) {  
                case 'facebook':
                    if (typeof FB != 'undefined') {
                        console.log('facebook');
                        FB.getLoginStatus(function(response) {
                            if (response.status == 'connected'){
                                var authResponse = response.authResponse;
                                data = {
                                    "username" : username.val(),
                                    "email": email.val(),
                                    "social_login_type": "facebook",
                                    "id": authResponse.userID,
                                    "access_token": authResponse.accessToken
                                }

                                if ($(agree_subscribe).is(":checked") === true) {
                                    data["agree_subscribe"] = true;
                                } else { data["agree_subscribe"] = false; }

                                // console.log(data);

                                if ($(agree_terms).is(":checked") === true) {
                                    $(slo_error).addClass('hidden');
                                    createAccountWithUsername(data);
                                } else {
                                    $(slo_error).html('<div class="alert alert-error m10">To continue you will have to accept the Terms and Conditions</div>').removeClass('hidden');
                                }
                            }
                        });
                    }
                break;
                case 'google':
                    console.log('google');
                    if (typeof auth2 != 'undefined') {
                        var authResponse = auth2.currentUser.get().getAuthResponse();
                        data = {
                            "username" : username.val(),
                            "social_login_type": "google",
                            "id_token": authResponse.id_token,
                            "access_token": authResponse.access_token
                        }

                        if ($(agree_subscribe).is(":checked") === true) {
                            data["agree_subscribe"] = true;
                        } else { data["agree_subscribe"] = false; }

                        // console.log(data);

                        //has agreed to the terms
                        if ($(agree_terms).is(":checked") === true) {
                            $(slo_error).addClass('hidden');

                            createAccountWithUsername(data);
                        } else {
                            $(slo_error).html('<div class="alert alert-error m10">To continue you will have to accept the Terms and Conditions</div>').removeClass('hidden');
                        }
                    }
                    console.log(auth2.currentUser);
                break;
            }
        });
    });

    $("#log_out").click(function(e){
        window.location.href = "/?status=loggedout";
    });
</script>
<!-- General Login Scripts End -->




